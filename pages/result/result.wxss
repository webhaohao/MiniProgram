/* pages/result/result.wxss */
.main{
    background:url('https://www.happyhao.top/bg3.png') no-repeat;
    width:100%;
    height:100%;
    background-size:100% 100%;
    position: fixed;
}
.title{
    margin-top:86rpx;
    text-align:center;
}
.title image{
    width:690rpx;    
}
.info{
    color:#185b6b;
    text-align:center;
    margin-top:27rpx;
}
.result{
    font-size:115rpx;
    color:#1f1f3b;
    text-align:center;
    letter-spacing:65rpx;
    padding:43rpx 0rpx;
}
.comment_con{
    margin-left:17rpx;
}
.comment_con image{
    width:678rpx;    
}
.comment_con{
    position: relative;
}
.comment_con text{
    position: absolute;
    width:460rpx;
    left:163rpx;
    top:26rpx;
    color:#fff;  
}
.msg {
    display:flex;
    justify-content:center;
    align-items:center;
    margin-top:78rpx;
}
.msg image{
    width:204rpx;    
}
.msg view:last-child{
    width:180rpx;
    margin-left:15rpx;
}
.msg view text{
    display:block;
    margin-bottom:15rpx;
    color:#1e1c3c;
    font-size:32rpx;
}
.create{
    display:flex;
    justify-content:center;
    margin-top:48rpx;
}
.create view{
    background:url('data:image/png;base64,');
    width:454rpx;
    height:70rpx;    
    line-height:70rpx;
    background-size:100% 100%;
    text-align:center;
    color:#fff;
}